<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.rumo.mapper.UserMapper" >
  
  	<select id="getById" resultType="com.rumo.pojo.User">
  		select id ,username ,password FROM tb_user where id = #{id}
  	</select>
  	
  	<select id="findUsers" resultType="com.rumo.pojo.User">
  		select id,username,password from tb_user 
  	</select>
  	
</mapper>